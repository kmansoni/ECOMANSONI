{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://mansoni.com/schemas/ws/KEY_ACK.schema.json",
  "title": "KEY_ACK",
  "type": "object",
  "additionalProperties": false,
  "required": ["roomId", "epoch", "fromDeviceId", "toDeviceId", "senderKeyId", "refId"],
  "properties": {
    "roomId": { "$ref": "https://mansoni.com/schemas/ws/types.schema.json#/properties/roomId" },
    "epoch": { "type": "integer", "minimum": 0 },
    "fromDeviceId": { "$ref": "https://mansoni.com/schemas/ws/types.schema.json#/properties/deviceId" },
    "toDeviceId": { "$ref": "https://mansoni.com/schemas/ws/types.schema.json#/properties/deviceId" },
    "senderKeyId": { "type": "string", "minLength": 8, "maxLength": 128 },
    "refId": { "type": "string", "format": "uuid", "description": "Acknowledges a prior message (typically REKEY_BEGIN or KEY_PACKAGE)" }
  }
}
